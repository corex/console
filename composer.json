{
    "name": "corex/console",
    "type": "library",
    "description": "Console framework (artisan, commands, visibility, recursive)",
    "license": "MIT",
    "keywords": [
        "console",
        "command"
    ],
    "authors": [
        {
            "name": "CoRex",
            "email": "dev@corex.dk"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/console": "^5.5",
        "illuminate/events": "^5.5",
        "corex/support": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "CoRex\\Console\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CoRex\\Console\\": "tests/"
        }
    }
}
